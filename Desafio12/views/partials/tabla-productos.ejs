<h2>Lista de productos</h2>

<% if(!listObject.state) { %>

    <h3><%=listObject.msg %></h3>

    <% } else { %>

        <table class="table">

            <thead>
                <tr>
                  <th scope="col">#id</th>
                  <th scope="col">Nombre</th>
                  <th scope="col">Precio</th>
                  <th scope="col">Icono/Imagen</th>
                </tr>
            </thead>

            <tbody id="tabla_body">

                <% listObject.list.forEach( function (product) { %>
                    <tr id="tabla_row">
                        <td><%=product.id %></td>
                        <td><%=product.title %></td>
                        <td><%=product.price %></td>
                        <td><img src="" alt="Imagen" srcset=<%=product.thumbnail %>></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    <% } %>